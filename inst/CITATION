pages = "196--216",

textVersion =
  paste("Wang, Lianming, and David B. Dunson.",
              "Fast Bayesian inference in Dirichlet process mixture models.",
              "Journal of Computational and Graphical Statistics, 20.1 (2011): 196-216."
  )

)
